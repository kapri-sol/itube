{
  "name": "itube",
  "version": "1.0.0",
  "description": "Server for the Youtube Cloen with GraphQL, Typescript, NodeJS",
  "main": "index.js",
  "repository": "https://github.com/kapri-sol/itube",
  "author": "kapri",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/node": "^12.7.3",
    "babel-runtime": "^6.26.0",
    "dotenv": "^8.1.0",
    "gql-merge": "^0.0.6",
    "graphql-to-typescript": "^0.1.2",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.2"
  },
  "scripts": {
    "deploy": "prisma deploy",
    "generate": "prisma generate",
    "prisma": "yarn run deploy && yarn run generate",
    "dev": "cd src && nodemon --exec ts-node index.ts -e ts, graphql",
    "pretypes": "gql-merge --out-file ./src/schema.graphql ./src/api/**/*.graphql",
    "types": "graphql-to-typescript ./src/schema.graphql ./src/types/graph.d.ts"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "cors": "^2.8.5",
    "graphql-tools": "^4.0.5",
    "graphql-yoga": "^1.18.3",
    "helmet": "^3.20.1",
    "merge-graphql-schemas": "^1.7.0",
    "morgan": "^1.9.1",
    "prisma-client-lib": "^1.34.8"
  }
}
